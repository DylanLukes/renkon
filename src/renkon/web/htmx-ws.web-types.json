{
  "$schema": "https://raw.githubusercontent.com/JetBrains/web-types/master/schema/web-types.json",
  "name": "htmx",
  "version": "1.0.0",
  "js-types-syntax": "typescript",
  "description-markup": "markdown",
  "contributions": {
    "html": {
      "attributes": [
        {
          "name": "ws-connect",
          "description": "The **ws-connect** attribute is used to establish a WebSocket connection to the specified URL.",
          "doc-url": "https://htmx.org/extensions/web-sockets/",
          "value": {
            "type": "string",
            "kind": "plain"
          }
        },
        {
          "name": "ws-send",
          "description": "The **ws-send** attribute is used to send a message over a WebSocket connection.",
          "doc-url": "https://htmx.org/extensions/web-sockets/",
          "value": {
            "kind": "no-value"
          }
        }
      ]
    },
    "css": {
      "classes": [
      ]
    }
  }
}